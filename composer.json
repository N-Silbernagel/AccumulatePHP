{
    "name": "nsilbernagel/accumulatephp",
    "description": "A PHP collections (accumulations) framework.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AccumulatePHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nils Silbernagel"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~9.5.26",
        "phpstan/phpstan": "~1.9.1",
        "ergebnis/phpstan-rules": "1.0.*",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "rector/rector": "~0.14.2"
    },
    "require": {
        "php": "~8.0 || ~8.1 || ~8.2",
        "jetbrains/phpstorm-attributes": "1.0"
    }
}
