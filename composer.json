{
    "name": "nsilbernagel/accumulatephp",
    "description": "A PHP collections (accumulations) framework.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AccumulatePHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nils Silbernagel"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6.3",
        "doctrine/instantiator": "^1.3.1",
        "phpstan/phpstan": "^1.9.17",
        "ergebnis/phpstan-rules": "^1.0.0",
        "phpstan/phpstan-deprecation-rules": "^1.1.1",
        "phpstan/phpstan-strict-rules": "^1.4.5",
        "rector/rector": "~0.15.13"
    },
    "require": {
        "php": "~8.0 || ~8.1 || ~8.2",
        "jetbrains/phpstorm-attributes": "1.0"
    }
}
